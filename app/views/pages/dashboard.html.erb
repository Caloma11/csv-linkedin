<div class="container">
  <h1>Dashboard</h1>
    <div>
      <%= simple_form_for(@task) do |f| %>
        <%= f.input :file, class: "btn btn-primary" %>
        <%= f.button :submit, "Convert", class: "btn-primary" %>
        <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
      <% end %>
    </div>
</div>
